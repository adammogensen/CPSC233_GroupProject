<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="800.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.MealGeneratorController">
   <children>
      <Label fx:id="greetMsgLabel" text="Greet message place holder.">
         <font>
            <Font size="14.0" />
         </font>
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
      </Label>
      <HBox alignment="CENTER_LEFT" prefHeight="145.0" prefWidth="800.0">
         <children>
            <VBox prefHeight="118.0" prefWidth="261.0">
               <children>
                  <Label prefHeight="20.0" prefWidth="178.0" text="Breakfaster Generator">
                     <font>
                        <Font size="15.0" />
                     </font>
                     <VBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </VBox.margin>
                  </Label>
                  <HBox prefHeight="44.0" prefWidth="259.0">
                     <children>
                        <Button fx:id="generateBFButton" mnemonicParsing="false" onAction="#userGenerateBF" text="Generate Breakfast">
                           <HBox.margin>
                              <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                           </HBox.margin>
                        </Button>
                        <Button fx:id="reGenerateBFButton" mnemonicParsing="false" onAction="#userReGenerateBF" prefHeight="23.0" prefWidth="85.0" text="Regenerate">
                           <HBox.margin>
                              <Insets top="10.0" />
                           </HBox.margin>
                        </Button>
                     </children>
                  </HBox>
               </children>
            </VBox>
            <Region prefHeight="191.0" prefWidth="216.0" />
            <VBox prefHeight="115.0" prefWidth="328.0">
               <children>
                  <HBox prefHeight="40.0" prefWidth="327.0">
                     <children>
                        <Label alignment="TOP_RIGHT" contentDisplay="RIGHT" text="Daily Recommanded Calories:">
                           <HBox.margin>
                              <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                           </HBox.margin>
                        </Label>
                        <Label fx:id="dailyCalLabel" alignment="TOP_RIGHT" contentDisplay="CENTER" prefHeight="15.0" prefWidth="72.0" text="PlaceHolder">
                           <HBox.margin>
                              <Insets bottom="10.0" left="2.0" right="10.0" top="10.0" />
                           </HBox.margin>
                        </Label>
                     </children>
                  </HBox>
                  <HBox prefHeight="100.0" prefWidth="200.0">
                     <children>
                        <Label text="Protein: ">
                           <HBox.margin>
                              <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                           </HBox.margin>
                        </Label>
                        <Label fx:id="gainerProteinLabel" text="PlaceHolder">
                           <HBox.margin>
                              <Insets bottom="10.0" left="2.0" right="10.0" top="10.0" />
                           </HBox.margin>
                        </Label>
                     </children>
                  </HBox>
                  <HBox prefHeight="100.0" prefWidth="200.0">
                     <children>
                        <Label prefHeight="15.0" prefWidth="76.0" text="BMR value:">
                           <HBox.margin>
                              <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                           </HBox.margin>
                        </Label>
                        <Label fx:id="gainerCarbHLabel" text="PlaceHolder">
                           <HBox.margin>
                              <Insets bottom="10.0" left="2.0" right="10.0" top="10.0" />
                           </HBox.margin>
                        </Label>
                     </children>
                  </HBox>
                  <HBox prefHeight="100.0" prefWidth="200.0">
                     <children>
                        <Label alignment="CENTER" text="BMI value:">
                           <HBox.margin>
                              <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                           </HBox.margin>
                        </Label>
                        <Label fx:id="gainerFatsLabel" text="PlaceHolder">
                           <HBox.margin>
                              <Insets bottom="10.0" left="2.0" right="10.0" top="10.0" />
                           </HBox.margin>
                        </Label>
                     </children>
                  </HBox>
               </children>
            </VBox>
         </children>
      </HBox>
      <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0">
         <children>
            <VBox prefHeight="100.0" prefWidth="280.0">
               <children>
                  <Label prefHeight="20.0" prefWidth="178.0" text="Breakfaster Generator">
                     <font>
                        <Font size="15.0" />
                     </font>
                     <VBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </VBox.margin>
                  </Label>
                  <Button fx:id="generateLunchButton" mnemonicParsing="false" onAction="#userGenerateLunch" text="Add meal">
                     <VBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </VBox.margin>
                  </Button>
               </children>
            </VBox>
            <Region prefHeight="100.0" prefWidth="200.0" />
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" />
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" />
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
         <children>
            <Button fx:id="addActivityButton" mnemonicParsing="false" onAction="#gainerAddActivity" text="Add activities">
               <HBox.margin>
                  <Insets bottom="10.0" left="100.0" right="10.0" top="10.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </children>
</VBox>
