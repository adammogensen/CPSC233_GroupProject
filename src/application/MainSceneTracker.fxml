<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.collections.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.VBox?>
<?import javafx.collections.FXCollections?>

<VBox xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.FirstSceneController">
   <children>
      <Label alignment="CENTER" prefHeight="0.0" prefWidth="529.0" text="Welcome to the Calories Tracker App">
         <font>
            <Font size="14.0" />
         </font></Label>
      <Label prefHeight="46.0" prefWidth="508.0" text="Please choose your active level in the box below,press done to continue.">
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <HBox prefHeight="100.0" prefWidth="200.0">
         <children>
            <Label prefHeight="20.0" prefWidth="98.0" text="Your choice: ">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <ChoiceBox fx:id="activeLevelChoiceBox" prefHeight="23.0" prefWidth="90.0">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
                <items>
             <FXCollections fx:factory="observableArrayList">
                   <String fx:value="Activist(one or more hour a day)" />
                   <String fx:value="Intermediate(half to one hour a day)" />
                   <String fx:value="Rookie(less than half hour a day)" />
                   <String fx:value="" />
             </FXCollections>
            </items>
            </ChoiceBox>
            <Button fx:id="firstSceneDone" mnemonicParsing="false" onAction="#FirstSceneDoneButton" text="Done">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
            </Button>
         </children>
         <VBox.margin>
            <Insets top="20.0" />
         </VBox.margin>
      </HBox>
      <Label fx:id="chooseNothingErrorLabel" alignment="CENTER" prefHeight="23.0" prefWidth="524.0" text=" ">
         <font>
            <Font size="14.0" />
         </font>
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
      </Label>
   </children>
</VBox>
